{
  "name": "react-transactions-provider",
  "version": "0.0.5",
  "description": "Transactions provider for React",
  "scripts": {
    "build": "npm run build:es && npm run build:commonjs",
    "build:es": "tsc -p ./tsconfig.es.json",
    "build:commonjs": "tsc -p ./tsconfig.commonjs.json",
    "clean": "rm -rf dist es node_modules",
    "prepublish": "npm run build"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "es/index.js",
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prilutskiy/react-transactions-provider.git"
  },
  "keywords": [
    "react",
    "command",
    "transaction",
    "undo"
  ],
  "author": "Max Prilutskiy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prilutskiy/react-transactions-provider/issues"
  },
  "homepage": "https://github.com/prilutskiy/react-transactions-provider#readme",
  "devDependencies": {
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.40",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "typescript": "^2.7.2"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "prop-types": "^15.6.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
